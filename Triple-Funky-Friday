})

local Tabs = {
    Main = Window:AddTab({ Title = "Main", Icon = "Shield" }),
    Credits = Window:AddTab({ Title = "Credits", Icon = "copyright" })
}

local Main = Tabs.Main:AddSection("Main")

Main:AddParagraph({
        Title = "Autoplayer",
        Content = "if the script won't execute or crashes you that means it wont support your executer"
    })

local Toggle = Tabs.Main:AddToggle("Autoplay", {Title = "Autoplay", Default = false })
